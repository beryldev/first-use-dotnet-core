								<ul class="dropdown-menu" uib-dropdown-menu aria-labelledby="simple-dropdown"> 
									<li><a ui-sref="documents.delivery">Delivery</a></li> 
									<li><a ui-sref="documents.relocation">Relocation</a></li>
									<li><a ui-sref="documents.release">Release</a></li>
								</ul> 
							</li>
						</ul>
					</div>			
				</div>
				<div class="col-sm-9 col-md-10">
					<div class="content-wrapper" ui-view="wrapper"></div>
				</div>
			</div>
		</div>
		

        <script src="build/js/lib.min.js"></script>
        <script src="build/js/app.min.js"></script>
