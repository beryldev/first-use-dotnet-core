{
    "version": "1.0.0-*",
    "testRunner": "xunit",
    "dependencies": {
        "Wrhs": "*",
        "Wrhs.Data": "*",
        "Wrhs.WebApp": "*",
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta2-build3300",
        "Moq": "4.6.38-alpha",
        "FluentAssertions": "4.17.0"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
