{
    "version": "1.0.0-*",
    "testRunner": "nunit",
    "dependencies": {
        "Wrhs": "*",
        "Wrhs.Data": "*",
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta2-build3300",
        "NUnit": "3.4.1",
        "dotnet-test-nunit": "3.4.0-beta-1",
        "Moq": "4.6.38-alpha"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "imports": ["dotnet5.6", "portable-net45+win8"],
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0-*"
                }
            }
        }
    },
    "buildOptions": {
        "copyToOutput": {
            "include": [ "xunit.runner.json" ]
        }
    },
    "tooling": {
        "defaultNamespace": "Wrhs.Tests"
    }
}
