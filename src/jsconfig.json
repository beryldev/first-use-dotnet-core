	],
	"exclude": [
		"Wrhs.WebApp/node_modules",
		"Wrhs.WebApp/bower_components",
		"Wrhs.WebApp/jspm_packages",
		"Wrhs.WebApp/tmp",
		"Wrhs.WebApp/temp",
        "Wrhs.WebApp/wwwroot/lib",
		"Wrhs.WebApp/wwwroot/build",
		"Wrhs.WebApp/bin",
		"Wrhs.WebApp/obj"
	]
}
